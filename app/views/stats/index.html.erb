
<script type="text/javascript" >
  $(function() {
     new Highcharts.Chart({
         chart: {
             renderTo: "chart_chart"
         },
         title: {
             text:"Incoming by day"
         },
         xAxis: {
            categories: [<%= @data_y.map { |x| "'#{x}'" }.join(',') %>]
         },
         yAxis: {
             title: {
                 text: "incoming"
             }
         },
         series: [{
             name: "Articles (in <%= l(@current_date, :format => :long_mon) %>)",
             data: <%= @data_x %>

         }]
     });
  });

</script>

<div id="chart_chart" style="width:560px; height:300px;">

</div>